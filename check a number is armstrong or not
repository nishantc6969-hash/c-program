// wap to check a number is  armstrong or not 
#include <stdio.h>
#include <math.h>
int main()
{
    int n,s=0,r,c;
    printf("enter a number:");
    scanf("%d",&n);
    c=n;

    while(n!=0)
    {
        r=n%10;
        s= s+ pow(r,3);
        n=n/10;

    }
    if (s==c){
        printf("%d is armstrong number\n",c);
    }
    else{
        printf("%d is not a armstrong number\n",c);
    }
    return 0;


}
