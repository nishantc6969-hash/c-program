// wap to check a number is palindrome or not 
#include <stdio.h>
int main()
{
    int n,rev=0,r,c;
    printf("enter a number:");
    scanf("%d",&n);
    c=n;

    while(n!=0)
    {
        r=n%10;
        rev=rev*10+r;;
        n=n/10;

    }
    if (rev==c){
        printf("%d is palindrome number\n",c);
    }
    else{
        printf("%d is not a palindrome number\n",c);
    }
    return 0;


}
