#include <stdio.h>

int main()
{
    int i=1, c=0, n;
    printf("enter a number ");
    scanf("%d",&n);
    while (i<= n){
        if (n%i==0){
            c=c+1;
        }
        i=i+1;
    }
    if (c==2){
        printf("anno. is prime");

    }
    else{
        printf("a no. is not prime ");
    }
    return 0;



}
